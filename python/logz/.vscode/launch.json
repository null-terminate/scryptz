{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${workspaceRoot}",
            "env": {
                "PYTHONPATH": "${workspaceRoot}"
            },
            "args": [
                "--start-time",
                "2025-09-11 07:00:00",
                "--end-time",
                "2025-09-11 08:00:00",
                "--query",
                "blah",
                "--chunk-minutes",
                "1",
                "--enable-deduplication"
            ]
        }
    ]
}